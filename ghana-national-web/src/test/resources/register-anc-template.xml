<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xf:xforms xmlns:xf="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" id="11">
    <xf:model>
        <xf:instance id="ANCRegistration">
            <ANCRegistration id="11" name="Reg-ANC">
                <staffId/>
                <facilityId/>
                <regDateToday/>
                <date/>
                <motechId/>
                <ancRegNumber/>
                <estDeliveryDate/>
                <deliveryDateConfirmed/>
                <height/>
                <gravida/>
                <parity/>
                <addHistory/>
                <addCareHistory/>
                <lastIPT/>
                <lastIPTDate/>
                <lastTT/>
                <lastTTDate/>
                <enroll/>
                <consent/>
                <ownership/>
                <phoneNumber/>
                <format/>
                <language/>
                <dayOfWeek/>
                <timeOfDay/>
                <learnedFrom/>
                <formtype>data_entry</formtype>
                <formname>registerANC</formname>
            </ANCRegistration>
        </xf:instance>
        <xf:instance id="format">
            <dynamiclist>
                <item id="PERS_VOICE" parent="PERSONAL">
                    <label>Voice</label>
                    <value>PERS_VOICE</value>
                </item>
                <item id="PERS_TEXT" parent="PERSONAL">
                    <label>SMS</label>
                    <value>PERS_TEXT</value>
                </item>
                <item id="HSE_VOICE" parent="HOUSEHOLD">
                    <label>Voice</label>
                    <value>HSE_VOICE</value>
                </item>
                <item id="HSE_TEXT" parent="HOUSEHOLD">
                    <label>SMS</label>
                    <value>HSE_TEXT</value>
                </item>
                <item id="PUB_VOICE" parent="PUBLIC">
                    <label>Voice</label>
                    <value>PUB_VOICE</value>
                </item>
            </dynamiclist>
        </xf:instance>
        <xf:instance id="language">
            <dynamiclist>
                <item id="EN" parent="PERS_VOICE">
                    <label>English</label>
                    <value>EN</value>
                </item>
                <item id="KAS" parent="PERS_VOICE">
                    <label>Kassim</label>
                    <value>KAS</value>
                </item>
                <item id="NAN" parent="PERS_VOICE">
                    <label>Nankam</label>
                    <value>NAN</value>
                </item>
                <item id="FAN" parent="PERS_VOICE">
                    <label>Fante</label>
                    <value>FAN</value>
                </item>
                <item id="EN" parent="PERS_TEXT">
                    <label>English</label>
                    <value>EN</value>
                </item>
                <item id="EN" parent="HSE_VOICE">
                    <label>English</label>
                    <value>EN</value>
                </item>
                <item id="KAS" parent="HSE_VOICE">
                    <label>Kassim</label>
                    <value>KAS</value>
                </item>
                <item id="NAN" parent="HSE_VOICE">
                    <label>Nankam</label>
                    <value>NAN</value>
                </item>
                <item id="FAN" parent="HSE_VOICE">
                    <label>Fante</label>
                    <value>FAN</value>
                </item>
                <item id="EN" parent="HSE_TEXT">
                    <label>English</label>
                    <value>EN</value>
                </item>
                <item id="EN" parent="PUB_VOICE">
                    <label>English</label>
                    <value>EN</value>
                </item>
                <item id="KAS" parent="PUB_VOICE">
                    <label>Kassim</label>
                    <value>KAS</value>
                </item>
                <item id="NAN" parent="PUB_VOICE">
                    <label>Nankam</label>
                    <value>NAN</value>
                </item>
                <item id="FAN" parent="PUB_VOICE">
                    <label>Fante</label>
                    <value>FAN</value>
                </item>
            </dynamiclist>
        </xf:instance>
        <xf:bind id="staffId" nodeset="/ANCRegistration/staffId" required="true()" type="xsd:int" constraint=". &lt; 2147483647" message="Number too large. Keep under 2147483647"/>
        <xf:bind id="facilityId" nodeset="/ANCRegistration/facilityId" required="true()" type="xsd:int" constraint=". &lt; 2147483647" message="Number too large. Keep under 2147483647"/>
        <xf:bind id="regDateToday" nodeset="/ANCRegistration/regDateToday" required="true()" type="xsd:string"/>
        <xf:bind constraint=". &lt;= 'today()'" id="date" message="Invalid date: date entered is in the future" relevant="/ANCRegistration/regDateToday = 'IN_PAST' or /ANCRegistration/regDateToday = 'IN_PAST_IN_OTHER_FACILITY'"
                 visible="false()" action="show" nodeset="/ANCRegistration/date" required="true()" type="xsd:date"/>
        <xf:bind id="motechId" nodeset="/ANCRegistration/motechId" required="true()" type="xsd:int" constraint=". &gt; 999999 and . &lt; 10000000" message="MoTeCH ID must be 7 digits long"/>
        <xf:bind id="ancRegNumber" nodeset="/ANCRegistration/ancRegNumber" type="xsd:string" required="true()"/>
        <xf:bind constraint=". &gt;= 'today()'" id="estDeliveryDate" message="Invalid EDD: date entered is in the past" nodeset="/ANCRegistration/estDeliveryDate" required="true()" type="xsd:date"/>
        <xf:bind id="deliveryDateConfirmed" nodeset="/ANCRegistration/deliveryDateConfirmed" required="true()" type="xsd:string"/>
        <xf:bind constraint=". &gt;= 60 and . &lt;= 200" id="height" message="Value out of range: must be between 60 and 200" nodeset="/ANCRegistration/height" required="true()" type="xsd:decimal"/>
        <xf:bind constraint=". &gt;= 1 and . &lt;= 30" id="gravida" message="Value out of range: must be between 1 and 30" nodeset="/ANCRegistration/gravida" required="true()" type="xsd:int"/>
        <xf:bind constraint=". &gt;= 0 and . &lt;= 30" id="parity" message="Value out of range: must be between 0 and 30" nodeset="/ANCRegistration/parity" required="true()" type="xsd:int"/>
        <xf:bind id="addHistory" nodeset="/ANCRegistration/addHistory" type="xsd:string" required="true()"/>
        <xf:bind id="addCareHistory" nodeset="/ANCRegistration/addCareHistory" relevant="/ANCRegistration/addHistory = '1'" type="xsd:string" visible="false()" action="show" required="true()"/>
        <xf:bind id="lastIPT" nodeset="/ANCRegistration/lastIPT" type="xsd:string" visible="false()" relevant="/ANCRegistration/addCareHistory = '1' and /ANCRegistration/addHistory = '1'"
                 action="show" required="true()"/>
        <xf:bind id="lastIPTDate" nodeset="/ANCRegistration/lastIPTDate" type="xsd:date" visible="false()" constraint=". &lt;= 'today()'" message="Invalid date: date specified is in the future"
                 relevant="/ANCRegistration/addCareHistory = '1' and /ANCRegistration/addHistory = '1'" action="show" required="true()"/>
        <xf:bind id="lastTT" nodeset="/ANCRegistration/lastTT" type="xsd:string" visible="false()" relevant="/ANCRegistration/addCareHistory = '2' and /ANCRegistration/addHistory = '1'"
                 action="show" required="true()"/>
        <xf:bind id="lastTTDate" nodeset="/ANCRegistration/lastTTDate" type="xsd:date" visible="false()" action="show" constraint=". &lt;= 'today()'"
                 message="Invalid date: date specified is in the future" relevant="/ANCRegistration/addCareHistory = '2' and /ANCRegistration/addHistory = '1'" required="true()"/>
        <xf:bind id="enroll" nodeset="/ANCRegistration/enroll" required="true()" type="xsd:string"/>
        <xf:bind action="show" id="consent" nodeset="/ANCRegistration/consent" relevant="/ANCRegistration/enroll = 'Y'" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="ownership" nodeset="/ANCRegistration/ownership" relevant="/ANCRegistration/consent = 'Y'" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="phoneNumber" nodeset="/ANCRegistration/phoneNumber" relevant="/ANCRegistration/ownership = 'PERSONAL' or /ANCRegistration/ownership = 'HOUSEHOLD'" required="true()"
                 type="xsd:int" visible="false()" constraint=". &lt;= 999999999" message="Number too large. It should be 10 digits long"/>
        <xf:bind action="show" id="format" nodeset="/ANCRegistration/format" relevant="/ANCRegistration/consent = 'Y'" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="language" nodeset="/ANCRegistration/language" relevant="/ANCRegistration/consent = 'Y'" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="dayOfWeek" nodeset="/ANCRegistration/dayOfWeek" relevant="/ANCRegistration/format = 'PERS_VOICE' or /ANCRegistration/format = 'HSE_VOICE'" required="true()"
                 type="xsd:string" visible="false()"/>
        <xf:bind action="show" id="timeOfDay" nodeset="/ANCRegistration/timeOfDay" relevant="/ANCRegistration/format = 'PERS_VOICE' or /ANCRegistration/format = 'HSE_VOICE'" required="true()"
                 type="xsd:time" visible="false()"/>
        <xf:bind action="show" id="learnedFrom" nodeset="/ANCRegistration/learnedFrom" relevant="/ANCRegistration/consent = 'Y'" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind id="formtype" nodeset="/ANCRegistration/formtype" required="true()" type="xsd:string" visible="false()"/>
        <xf:bind id="formname" nodeset="/ANCRegistration/formname" required="true()" type="xsd:string" visible="false()"/>
    </xf:model>
    <xf:group id="1">
        <xf:label>Page1</xf:label>
        <xf:input bind="staffId">
            <xf:label>Staff ID</xf:label>
        </xf:input>
        <xf:input bind="facilityId">
            <xf:label>Facility ID</xf:label>
        </xf:input>
        <xf:select1 bind="regDateToday">
            <xf:label>ANC Registration Today?</xf:label>
            <xf:item id="0">
                <xf:label>Today</xf:label>
                <xf:value>TODAY</xf:value>
            </xf:item>
            <xf:item id="1">
                <xf:label>In the past</xf:label>
                <xf:value>IN_PAST</xf:value>
            </xf:item>
            <xf:item id="2">
                <xf:label>In the past in other facility</xf:label>
                <xf:value>IN_PAST_IN_OTHER_FACILITY</xf:value>
            </xf:item>
        </xf:select1>
        <xf:input bind="date">
            <xf:label>Reg. date</xf:label>
        </xf:input>
        <xf:input bind="motechId">
            <xf:label>MoTeCH ID</xf:label>
        </xf:input>
        <xf:input bind="ancRegNumber">
            <xf:label>Serial Number</xf:label>
        </xf:input>
        <xf:input bind="estDeliveryDate">
            <xf:label>EDD</xf:label>
        </xf:input>
        <xf:select1 bind="deliveryDateConfirmed">
            <xf:label>Delivery date confirmed by CHW</xf:label>
            <xf:item id="Y">
                <xf:label>Yes</xf:label>
                <xf:value>Y</xf:value>
            </xf:item>
            <xf:item id="N">
                <xf:label>No</xf:label>
                <xf:value>N</xf:value>
            </xf:item>
        </xf:select1>
        <xf:input bind="height">
            <xf:label>Height (cm)</xf:label>
        </xf:input>
        <xf:input bind="gravida">
            <xf:label>Gravida</xf:label>
        </xf:input>
        <xf:input bind="parity">
            <xf:label>Parity</xf:label>
        </xf:input>
        <xf:select1 bind="addHistory">
            <xf:label>Add History</xf:label>
            <xf:item id="1">
                <xf:label>Yes</xf:label>
                <xf:value>1</xf:value>
            </xf:item>
            <xf:item id="2">
                <xf:label>No</xf:label>
                <xf:value>0</xf:value>
            </xf:item>
        </xf:select1>
        <xf:select bind="addCareHistory">
            <xf:label>Add Care History</xf:label>
            <xf:item id="1">
                <xf:label>IPT</xf:label>
                <xf:value>1</xf:value>
            </xf:item>
            <xf:item id="2">
                <xf:label>TT</xf:label>
                <xf:value>2</xf:value>
            </xf:item>
        </xf:select>
        <xf:select1 bind="lastIPT">
            <xf:label>Last IPT</xf:label>
            <xf:item id="1">
                <xf:label>IPT 1</xf:label>
                <xf:value>1</xf:value>
            </xf:item>
            <xf:item id="2">
                <xf:label>IPT 2</xf:label>
                <xf:value>2</xf:value>
            </xf:item>
            <xf:item id="3">
                <xf:label>IPT 3</xf:label>
                <xf:value>3</xf:value>
            </xf:item>
        </xf:select1>
        <xf:input bind="lastIPTDate">
            <xf:label>Last IPT date</xf:label>
        </xf:input>
        <xf:select1 bind="lastTT">
            <xf:label>Last TT</xf:label>
            <xf:item id="1">
                <xf:label>TT 1</xf:label>
                <xf:value>1</xf:value>
            </xf:item>
            <xf:item id="2">
                <xf:label>TT 2</xf:label>
                <xf:value>2</xf:value>
            </xf:item>
            <xf:item id="3">
                <xf:label>TT 3</xf:label>
                <xf:value>3</xf:value>
            </xf:item>
            <xf:item id="4">
                <xf:label>TT 4</xf:label>
                <xf:value>4</xf:value>
            </xf:item>
            <xf:item id="5">
                <xf:label>TT 5</xf:label>
                <xf:value>5</xf:value>
            </xf:item>
        </xf:select1>
        <xf:input bind="lastTTDate">
            <xf:label>Last TT Date</xf:label>
        </xf:input>
        <xf:select1 bind="enroll">
            <xf:label>Join Mobile Midwife?</xf:label>
            <xf:item id="Y">
                <xf:label>Yes</xf:label>
                <xf:value>Y</xf:value>
            </xf:item>
            <xf:item id="N">
                <xf:label>No</xf:label>
                <xf:value>N</xf:value>
            </xf:item>
        </xf:select1>
        <xf:select1 bind="consent">
            <xf:label>Consent?</xf:label>
            <xf:item id="Y">
                <xf:label>Yes</xf:label>
                <xf:value>Y</xf:value>
            </xf:item>
            <xf:item id="N">
                <xf:label>No</xf:label>
                <xf:value>N</xf:value>
            </xf:item>
        </xf:select1>
        <xf:select1 bind="ownership">
            <xf:label>Phone ownership</xf:label>
            <xf:item id="PERSONAL">
                <xf:label>Personal phone</xf:label>
                <xf:value>PERSONAL</xf:value>
            </xf:item>
            <xf:item id="HOUSEHOLD">
                <xf:label>Household phone</xf:label>
                <xf:value>HOUSEHOLD</xf:value>
            </xf:item>
            <xf:item id="PUBLIC">
                <xf:label>Public phone</xf:label>
                <xf:value>PUBLIC</xf:value>
            </xf:item>
        </xf:select1>
        <xf:input bind="phoneNumber">
            <xf:label>Phone number</xf:label>
        </xf:input>
        <xf:select1 bind="format">
            <xf:label>Message format</xf:label>
            <xf:itemset nodeset="instance('format')/item[@parent=instance('ANCRegistration')/ownership]">
                <xf:label ref="label"/>
                <xf:value ref="value"/>
            </xf:itemset>
        </xf:select1>
        <xf:select1 bind="language">
            <xf:label>Message language</xf:label>
            <xf:itemset nodeset="instance('language')/item[@parent=instance('ANCRegistration')/format]">
                <xf:label ref="label"/>
                <xf:value ref="value"/>
            </xf:itemset>
        </xf:select1>
        <xf:select1 bind="dayOfWeek">
            <xf:label>Day of week to receive messages</xf:label>
            <xf:item id="monday">
                <xf:label>Monday</xf:label>
                <xf:value>monday</xf:value>
            </xf:item>
            <xf:item id="tuesday">
                <xf:label>Tuesday</xf:label>
                <xf:value>tuesday</xf:value>
            </xf:item>
            <xf:item id="wednesday">
                <xf:label>Wednesday</xf:label>
                <xf:value>wednesday</xf:value>
            </xf:item>
            <xf:item id="thursday">
                <xf:label>Thursday</xf:label>
                <xf:value>thursday</xf:value>
            </xf:item>
            <xf:item id="friday">
                <xf:label>Friday</xf:label>
                <xf:value>friday</xf:value>
            </xf:item>
            <xf:item id="saturday">
                <xf:label>Saturday</xf:label>
                <xf:value>saturday</xf:value>
            </xf:item>
            <xf:item id="sunday">
                <xf:label>Sunday</xf:label>
                <xf:value>sunday</xf:value>
            </xf:item>
        </xf:select1>
        <xf:input bind="timeOfDay">
            <xf:label>Time of day to receive messages</xf:label>
        </xf:input>
        <xf:select1 bind="learnedFrom">
            <xf:label>How learned of Mobile Midwife</xf:label>
            <xf:item id="GHS_NURSE">
                <xf:label>GHS Nurse</xf:label>
                <xf:value>GHS_NURSE</xf:value>
            </xf:item>
            <xf:item id="MOTECH_FIELD_AGENT">
                <xf:label>MoTeCH field agent</xf:label>
                <xf:value>MOTECH_FIELD_AGENT</xf:value>
            </xf:item>
            <xf:item id="FRIEND">
                <xf:label>Friend</xf:label>
                <xf:value>FRIEND</xf:value>
            </xf:item>
            <xf:item id="POSTERS_ADS">
                <xf:label>Posters/ads</xf:label>
                <xf:value>POSTERS_ADS</xf:value>
            </xf:item>
            <xf:item id="RADIO">
                <xf:label>Radio</xf:label>
                <xf:value>RADIO</xf:value>
            </xf:item>
        </xf:select1>
        <xf:input bind="formtype">
            <xf:label>formType</xf:label>
        </xf:input>
        <xf:input bind="formname">
            <xf:label>formName</xf:label>
        </xf:input>
    </xf:group>
</xf:xforms>